{
	"name" : "luyadev/luya-mailjet",
	"type" : "luya-extension",
	"keywords" : [
		"luya", "mailjet", "yii2", "yii"
	],
	"license" : "MIT",
	"authors" : [
		{
			"name" : "Basil Suter",
			"email" : "basil@nadar.io"
		}
	],
	"require": {
		"luyadev/luya-core" : "^1.0.25",
		"mailjet/mailjet-apiv3-php": "^1.3"
	},
	"require-dev" : {
		"luyadev/luya-testsuite" : "^2.0",
		"vlucas/phpdotenv": "^2.5"
	},
	"autoload" : {
		"psr-4" : {
			"luya\\mailjet\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"luya\\mailjet\\tests\\" : "tests/"
		}
	},
	"config": {
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
